package main

import(
   "driver"
   //"network"
   "iomodule"
   "fmt"
   "heis"
   "time"
)



func main(){
   
   order_queue := make(chan driver.Data, 1)
   command_list := make(chan driver.Data, 1)
   order_list := make(chan driver.Data, 1)
   cost := make(chan driver.Data, 1)
   remove_order := make(chan driver.Data, 1)
   remove_command :=  make(chan driver.Data, 1)
   driver.Init()
   fmt.Printf("Iomodule i choose you\n")
   go iomodule.IoManager(order_queue, command_list, order_list, cost, remove_order, remove_command)
   fmt.Printf("Heis i choose you\n")
   go heis.Heis(order_list, command_list, cost, remove_order, remove_command)
  
   for {
      
      time.Sleep(time.Second)
   }
   
}
